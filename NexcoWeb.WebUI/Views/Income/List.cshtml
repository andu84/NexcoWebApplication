@model NexcoWeb.WebUI.Models.IncomesListViewModel
@using NexcoWeb.WebUI.HtmlHelpers;

@{
    ViewBag.Title = "Incomes";
}



@foreach (var i in Model.Incomes)
{
    @Html.Partial("IncomeSummary", i)
}

@Html.ActionLink("Create new Incomes", "../Admin/Create", null, new { @class = "btn btn-info pull-left" })
<div class="btn-group pull-right">
    @Html.PageLinks(Model.PagingInfo, x => Url.Action("List", new { page = x, description = Model.CurrentDescription }))
</div>
