@model NexcoWeb.Domain.Entities.Expenditure

    <tr>
        <td>
            <strong> @Model.DisplayDateExpenses</strong>
        </td>
        <td>
            <strong>@Model.Travel</strong>
        </td>
        <td>
            <strong>@Model.Food</strong>
        </td>
        <td>
            <strong>@Model.Auto</strong>
        </td>
        <td>
            <strong>@Model.Entertaiment</strong>
        </td>
        <td>
            <strong>@Model.HouseholdExpenses</strong>
        </td>
        <td>
            <strong>@Model.Clothing</strong>
        </td>
        <td>
            <strong>@Model.Loan</strong>
        </td>
        <td>
            <strong>@Model.OtherExpenses</strong>
        </td>
        <td>
            <strong>@Model.TotalExpense</strong>
        </td>
        <td>
            <strong>@Model.DescriptionExpenditure</strong>
        </td>
        <td>
            <div class="btn btn-info btn-xs">
                @Html.ActionLink("Edit", "../AdminExpenditure/EditExpenditure", new { expenditureId = @Model.ExpenditureId })
            </div>
        </td>
        <td>
            @using (Html.BeginForm("DeleteExpenditure", "AdminExpenditure"))
            {
                @Html.Hidden("ExpenditureId", @Model.ExpenditureId)
                <input type="submit" class="btn btn-info btn-xs"
                       value="Delete" />
            }

        </td>
    </tr>

